{
  "name": "CPDBFlux",
  "version": "0.0.3",
  "description": "Example Flux architecture.",
  "repository": "https://github.com/facebook/flux",
  "main": "allegation/static/allegation/js/app.js",
  "dependencies": {
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "leaflet.heat": "~0.1.3",
    "leaflet.markercluster": "0.4.0",
    "leaflet.heat": "~0.1.3",
    "moment": "^2.10.3",
    "object-assign": "^1.0.0",
    "react": "^0.13.0",
    "chart.js": "~1.0.2",
    "react-chartjs": "~0.6.0",
    "react-tools": "~0.13.3",
    "react-mini-router": "~1.1.7",
    "pluralize": "~1.1.2",
    "lodash": "~3.10.0",
    "q": "~1.4.1",
    "jquery": "~2.1.4"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "jest-cli": "^0.4.3",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "scripts": {
    "start": "watchify -o allegation/static/allegation/js/bundle.js -v -d allegation/static/allegation/js/app.js",
    "start_admin": "watchify -o allegation/static/admin/js/bundle.js -v -d allegation/static/admin/js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > allegation/static/allegation/js/bundle.min.js",
    "build_admin": "browserify allegation/static/admin/js/app.js -t [envify --NODE_ENV production] | uglifyjs -cm > allegation/static/admin/js/bundle.min.js",
    "circleci": "browserify allegation/static/allegation/js/app.js > allegation/static/allegation/js/bundle.js",
    "test": "jest"
  },
  "author": "Stefan Georg",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "jest": {
    "rootDir": "allegation/static/allegation/js",
    "scriptPreprocessor": "<rootDir>/test-preprocessor.js"
  }
}
